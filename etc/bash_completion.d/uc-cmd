_uc-cmd_log() {
  local cur prev words cword
  _init_completion || return
  if [[ $cword -eq 2 ]]; then
    COMPREPLY=( $( compgen -W 'critical debug error get info rotate warning' -- "$cur" ))
  else
    COMPREPLY=( $( compgen -W '$(ls)' -- $cur ))
  fi
}

_uc-cmd() {
  local cur prev words cword
  _init_completion || return
  if [[ $cword -eq 1 ]]; then
    COMPREPLY=( $( compgen -W 'action clone cmd config create cvar debug destroy device driver dump edit file history key list lock log lpi modbus modbus-slave owfs phi save server state test unlock update user' -- "$cur" ))
  else
    case "${words[1]}" in
      action|clone|config|cvar|device|driver|edit|file|key|log|lpi|modbus|modbus-slave|owfs|phi|server|user)
        _uc-cmd_${words[1]}
        ;;
      *)
        COMPREPLY=( $( compgen -W '$(ls)' -- $cur ))
        ;;
    esac
  fi
}

complete -F _uc-cmd uc-cmd