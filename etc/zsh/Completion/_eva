#compdef eva

_eva_system() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 2 ]]; then
    compadd "$@" poweroff reboot
  else
    _files
  fi
}

_eva_iote() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 2 ]]; then
    compadd "$@" join leave list
  else
    _files
  fi
}

_eva_sfa_server() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" cleanup launch plugins reload restart start status stop
  else
    _files
  fi
}

_eva_sfa_corescript() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" delete edit list mqtt-subscribe mqtt-topics mqtt-unsubscribe reload
  else
    _files
  fi
}

_eva_sfa_cloud() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" deploy undeploy update
  else
    _files
  fi
}

_eva_sfa_controller() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" append disable enable list ma-test props reload remove set test upnp-rescan
  else
    _files
  fi
}

_eva_sfa_supervisor() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" lock message unlock
  else
    _files
  fi
}

_eva_sfa_notify() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" reload restart
  else
    _files
  fi
}

_eva_sfa_edit() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" corescript pvt server-config ui
  else
    _files
  fi
}

_eva_sfa_cycle() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" list
  else
    _files
  fi
}

_eva_sfa_macro() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" list result run
  else
    _files
  fi
}

_eva_sfa_action() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" clear disable enable exec kill result terminate toggle
  else
    _files
  fi
}

_eva_sfa_user() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" create destroy key list password
  else
    _files
  fi
}

_eva_sfa_key() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" create destroy list props regenerate set
  else
    _files
  fi
}

_eva_sfa_file() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" create get mod unlink upload
  else
    _files
  fi
}

_eva_sfa_cvar() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" delete get list set
  else
    _files
  fi
}

_eva_sfa_log() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 3 ]]; then
    compadd "$@" api critical debug error get info warning
  else
    _files
  fi
}

_eva_sfa() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 2 ]]; then
    compadd "$@" action clear cloud cmd controller corescript cvar cycle debug dump edit file history key lock log macro notify remote reset save server set slog state supervisor test toggle unlock user watch
  else
    case "${words[3]}" in
      action|cloud|controller|corescript|cvar|cycle|edit|file|key|log|macro|notify|server|supervisor|user)
        _eva_sfa_${words[3]}
        ;;
      *)
        _files
        ;;
    esac
  fi
}

