_eva_uc() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 2 ]]; then
    compadd "$@" action clone cmd config corescript create cvar datapuller debug destroy device driver dump edit file history key list lock log lpi maintenance modbus modbus-slave owfs phi save server slog state test unlock update user watch
  else
    case "${words[3]}" in
      action|clone|config|corescript|cvar|datapuller|device|driver|edit|file|key|log|lpi|maintenance|modbus|modbus-slave|owfs|phi|server|user)
        _eva_uc_${words[3]}
        ;;
      *)
        _files
        ;;
    esac
  fi
}

_eva_masterkey() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 2 ]]; then
    compadd "$@" set
  else
    _files
  fi
}

_eva_edit() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 2 ]]; then
    compadd "$@" crontab
  else
    _files
  fi
}

_eva_backup() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 2 ]]; then
    compadd "$@" list restore save unlink
  else
    _files
  fi
}

_eva_server() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 2 ]]; then
    compadd "$@" disable enable restart start status stop
  else
    _files
  fi
}

_eva_mirror() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 2 ]]; then
    compadd "$@" set update
  else
    _files
  fi
}

_eva() {
  local cword
  let cword=CURRENT-1
  if [[ $cword -eq 1 ]]; then
    compadd "$@" backup edit iote lm masterkey mirror ns save server sfa system uc update version
  else
    case "${words[2]}" in
      backup|edit|iote|lm|masterkey|mirror|server|sfa|system|uc)
        _eva_${words[2]}
        ;;
      *)
        _files
        ;;
    esac
  fi
}
